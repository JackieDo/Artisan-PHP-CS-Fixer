{
    "name": "jackiedo/artisan-php-cs-fixer",
    "description": "A PHP-CS-Fixer bridge for use via Artisan CLI on Laravel.",
    "keywords": ["laravel", "artisan", "develop", "php-cs-fixer", "php", "coding", "standards", "fixer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jackie Do",
            "email": "anhvudo@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "jackiedo/path-helper": "^1.0"
    },
    "require-dev": {
        "illuminate/console": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Jackiedo\\ArtisanPhpCsFixer\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "laravel": {
            "providers": [
                "Jackiedo\\ArtisanPhpCsFixer\\ArtisanPhpCsFixerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
